import java.util.Scanner;
public class rev
{
    public static void main(String[] args) 
{
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Initial Value:");
        int n = sc.nextInt();
        System.out.println("Enter Range Value:");
        int m = sc.nextInt();
        int i = n;
        while(i>=m)
{
        System.out.println(i);
        i=i-2;

}
		
}
}

output:
Enter Initial Value:
80
Enter Range value:
60
80
78
76
74
72
70
68
66
64
62
60
