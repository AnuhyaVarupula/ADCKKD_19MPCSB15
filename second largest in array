import java.util.*;
class second
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
 System.out.print("Enter the size of array:");
int n=sc.nextInt();
 System.out.print("Enter the elements  of array:");
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
Arrays.sort(a);
int b= a[n-2];
System.out.print("2nd largest element is : "+b );
}
}
